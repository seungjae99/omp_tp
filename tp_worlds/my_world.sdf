        <!-- Obstacle -->
        <model name="obstacle">
            <static>true</static>
            <pose>5 0 0 0 0 0</pose>
            <link name="base_link">
                <visual name="obstacle">
                    <geometry>
                        <sphere>
                            <radius>0.5</radius>
                        </sphere>
                    </geometry>
                    <material>
                        <ambient>1 0 0 1</ambient>
                        <diffuse>1 0 0 1</diffuse>
                        <specular>1 0 0 1</specular>
                    </material>
                </visual>
            </link>
        </model>

